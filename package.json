{
  "name": "spreadsheettest",
  "version": "1.0.0",
  "description": "SpreadsheetをGoogleAppsScriptでいろいろコネコネするテスト",
  "main": "index.js",
  "scripts": {
    "lint": "tslint --fix src/**/*.ts",
    "test": "jest",
    "build": "npm run lint && npm run test && rimraf dist && webpack && cpx src/appsscript.json dist",
    "deploy": "npm run build && clasp push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tamurashingo/spreadsheet_test.git"
  },
  "author": "tamura shingo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tamurashingo/spreadsheet_test/issues"
  },
  "homepage": "https://github.com/tamurashingo/spreadsheet_test#readme",
  "devDependencies": {
    "@types/google-apps-script": "0.0.25",
    "@types/jest": "^23.3.0",
    "cpx": "^1.5.0",
    "gas-webpack-plugin": "^0.3.0",
    "jest": "^23.4.1",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.0.1",
    "ts-loader": "^4.4.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.9.2",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(test/.+(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
